<%= form_tag(redirect_path, method: :get, class: "form-inline") do %>
  Start Date: <%= date_field_tag :start_date, "", class: 'datepicker form-control', placeholder: 'mm/dd/yyyy', required: true %>
  End Date: <%= date_field_tag :end_date, "", class: 'datepicker form-control', placeholder: 'mm/dd/yyyy', required: true %>
  <%= submit_tag 'Set Report Date Range', class: 'btn btn-primary' %>
  <%= link_to 'reset date range', redirect_path if params[:start_date] %>
<% end %>