<div class="modal fade" id="collections-to-delete-deny-modal" tabindex="-1" role="dialog" aria-labelledby="delete-collection-label">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <form class="delete-collection-form">
        <div class="modal-body">
          <p>You must select one or more collections to delete.</p>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-primary" data-dismiss="modal"><%= t('helpers.action.close') %></button>
        </div>
      </form>
    </div>
  </div>
</div>
