<div class="row">
  <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title"><%= t('.title') %></h3>
      </div>
      <div class="panel-body">
        <table id="analytics-collections-table" class="table data-table table-striped">
          <thead>
          <tr>
            <th><%= t('.collection') %></th>
            <th><%= t('.last_updated') %></th>
            <th><%= t('.works') %></th>
            <th><%= t('.files') %></th>
          </tr>
          </thead>
          <tbody>
          <% @collection_rows.each do |row| %>
            <tr>
              <td><%= row.collection_name %></td>
              <td><%= row.updated %></td>
              <td><%= row.work_count %></td>
              <td><%= row.file_count %></td>
            </tr>
          <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>