<h1><%= t("hyrax.file_manager.link_text") %></h1>
<ul class="breadcrumb">
  <li class="breadcrumb-item">
    <%= t('.back_to') %> <%= link_to @form.to_s, [main_app, @form] %>
  </li>
</ul>

<% if !@form.member_presenters.blank? %>
  <div class="row" data-action="file-manager">
    <div class="col-lg-3" id="file-manager-tools">
      <h2><%= t('.toolbar') %></h2>
      <%= render "file_manager_actions" %>
    </div>
    <div class="col-lg-9" id="order-grid">
      <%= render "file_manager_members" %>
    </div>
  </div>
<% end %>
<div id="file-manager-extra-tools">
  <%= render "file_manager_extra_tools" %>
</div>
